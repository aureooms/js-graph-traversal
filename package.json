{
  "name": "@aureooms/js-graph-traversal",
  "description": "Graph traversal code bricks for JavaScript",
  "version": "0.1.0",
  "author": "aureooms",
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ],
    "timeout": "1m"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-async-generator-functions"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-graph-traversal/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@aureooms/js-adjacency-list": "^4.0.7",
    "@aureooms/js-collections": "^4.0.0",
    "@aureooms/js-dll": "^7.0.0",
    "@aureooms/js-graph-augment": "^0.1.0",
    "@aureooms/js-graph-labeled": "^0.1.1",
    "@aureooms/js-graph-sugar": "^0.1.2",
    "@aureooms/js-itertools": "^4.0.0",
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.6",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.9.6",
    "@babel/register": "7.9.0",
    "ava": "3.8.2",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "np": "6.2.3",
    "nyc": "15.0.1"
  },
  "files": [
    "lib"
  ],
  "homepage": "http://aureooms.github.io/js-graph-traversal/",
  "keywords": [
    "bricks",
    "ender",
    "graph",
    "javascript",
    "js",
    "traversal"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "url": "https://github.com/aureooms/js-graph-traversal.git",
    "type": "git"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "esdoc": "esdoc",
    "prepare": "npm run build",
    "release": "np",
    "test": "ava"
  }
}
